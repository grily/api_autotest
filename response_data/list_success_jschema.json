{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "code": {
      "type": "integer"
    },
    "message": {
      "type": "string"
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "title": {
            "type": "string"
          },
          "category": {
            "type": "integer"
          },
          "imgUrl": {
            "type": "string"
          },
          "contents": {
            "type": ["string","integer"]
          },
          "keywords": {
            "type": "string"
          },
          "vip": {
            "type": "integer"
          },
          "actionType": {
            "type": "integer"
          },
          "sort": {
            "type": "integer"
          },
          "url": {
            "type": [
              "null",
              "string"
            ]
          },
          "dataVersion": {
            "type": "integer"
          },
          "subjectId": {
            "type": "integer"
          },
          "gradePhase": {
            "type": "integer"
          },
          "updated": {
            "type": "integer"
          },
          "introduction": {
            "type": ["null","string","integer"]
          },
          "tag": {
            "type": "array"
          },
          "operatelog": {
            "type": "array"
          },
          "operate": {
            "type": ["object","null"],
            "properties": {
              "id": {
                "type": "integer"
              },
              "experiment_id": {
                "type": "integer"
              },
              "hits_count": {
                "type": "integer"
              },
              "real_hits_count": {
                "type": "integer"
              },
              "last_real_hits_count": {
                "type": "integer"
              },
              "play_count": {
                "type": "integer"
              },
              "real_play_count": {
                "type": "integer"
              },
              "last_real_play_count": {
                "type": "integer"
              },
              "share_count": {
                "type": "integer"
              },
              "real_share_count": {
                "type": "integer"
              },
              "last_real_share_count": {
                "type": "integer"
              },
              "likes_count": {
                "type": "integer"
              },
              "real_likes_count": {
                "type": "integer"
              },
              "last_real_likes_count": {
                "type": "integer"
              },
              "created_at": {
                "type": "integer"
              },
              "updated_at": {
                "type": "integer"
              },
              "auto_update_at": {
                "type": "string"
              }
            },
            "required": [
              "auto_update_at",
              "created_at",
              "experiment_id",
              "hits_count",
              "id",
              "last_real_hits_count",
              "last_real_likes_count",
              "last_real_play_count",
              "last_real_share_count",
              "likes_count",
              "play_count",
              "real_hits_count",
              "real_likes_count",
              "real_play_count",
              "real_share_count",
              "share_count",
              "updated_at"
            ]
          }
        },
        "required": [
          "actionType",
          "category",
          "contents",
          "dataVersion",
          "gradePhase",
          "id",
          "imgUrl",
          "introduction",
          "keywords",
          "operate",
          "operatelog",
          "sort",
          "subjectId",
          "tag",
          "title",
          "updated",
          "url",
          "vip"
        ]
      }
    },
    "_links": {
      "type": "object",
      "properties": {
        "self": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          },
          "required": [
            "href"
          ]
        },
        "first": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          },
          "required": [
            "href"
          ]
        },
        "last": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          },
          "required": [
            "href"
          ]
        }
      },
      "required": [
        "first",
        "last",
        "self"
      ]
    },
    "_meta": {
      "type": "object",
      "properties": {
        "totalCount": {
          "type": "integer"
        },
        "pageCount": {
          "type": "integer"
        },
        "currentPage": {
          "type": "integer"
        },
        "perPage": {
          "type": "integer"
        }
      },
      "required": [
        "currentPage",
        "pageCount",
        "perPage",
        "totalCount"
      ]
    },
    "_extra": {
      "type": "object",
      "properties": {
        "realTotalCount": {
          "type": "integer"
        }
      },
      "required": [
        "realTotalCount"
      ]
    }
  },
  "required": [
    "_extra",
    "_links",
    "_meta",
    "code",
    "data",
    "message"
  ]
}