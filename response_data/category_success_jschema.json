{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "code": {
      "type": "integer"
    },
    "message": {
      "type": "string"
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "subjectId": {
            "type": "integer"
          },
          "gradePhase": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "number": {
            "type": "integer"
          },
          "sort": {
            "type": "integer"
          },
          "created_at": {
            "type": "integer"
          },
          "updated_at": {
            "type": "integer"
          },
          "auto_update_at": {
            "type": "string"
          }
        },
        "required": [
          "auto_update_at",
          "created_at",
          "gradePhase",
          "id",
          "name",
          "number",
          "sort",
          "subjectId",
          "updated_at"
        ]
      }
    }
  },
  "required": [
    "code",
    "data",
    "message"
  ]
}