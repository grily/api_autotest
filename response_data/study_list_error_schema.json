{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "code": {
      "type": "integer"
    },
    "message": {
      "type": "string"
    },
    "data": {
      "type": "array"
    },
    "_links": {
      "type": "object",
      "properties": {
        "self": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          },
          "required": [
            "href"
          ]
        }
      },
      "required": [
        "self"
      ]
    },
    "_meta": {
      "type": "object",
      "properties": {
        "totalCount": {
          "type": "integer"
        },
        "pageCount": {
          "type": "integer"
        },
        "currentPage": {
          "type": "integer"
        },
        "perPage": {
          "type": "integer"
        }
      },
      "required": [
        "currentPage",
        "pageCount",
        "perPage",
        "totalCount"
      ]
    }
  },
  "required": [
    "_links",
    "_meta",
    "code",
    "data",
    "message"
  ]
}