{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "code": {
      "type": "integer"
    },
    "message": {
      "type": "string"
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "user_id": {
            "type": "integer"
          },
          "msg_id": {
            "type": "integer"
          },
          "is_read": {
            "type": "integer"
          },
          "created_at": {
            "type": "integer"
          },
          "subject": {
            "type": "string"
          },
          "display_content": {
            "type": "string"
          },
          "type": {
            "type": "integer"
          },
          "title": {
            "type": "string"
          },
          "grade_phase_id": {
            "type": "integer"
          },
          "class_id": {
            "type": "integer"
          }
        },
        "required": [
          "class_id",
          "created_at",
          "display_content",
          "grade_phase_id",
          "id",
          "is_read",
          "msg_id",
          "subject",
          "title",
          "type",
          "user_id"
        ]
      }
    },
    "_links": {
      "type": "object",
      "properties": {
        "self": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          },
          "required": [
            "href"
          ]
        },
        "first": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          },
          "required": [
            "href"
          ]
        },
        "last": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          },
          "required": [
            "href"
          ]
        }
      },
      "required": [
        "first",
        "last",
        "self"
      ]
    },
    "_meta": {
      "type": "object",
      "properties": {
        "totalCount": {
          "type": "integer"
        },
        "pageCount": {
          "type": "integer"
        },
        "currentPage": {
          "type": "integer"
        },
        "perPage": {
          "type": "integer"
        }
      },
      "required": [
        "currentPage",
        "pageCount",
        "perPage",
        "totalCount"
      ]
    }
  },
  "required": [
    "_links",
    "_meta",
    "code",
    "data",
    "message"
  ]
}